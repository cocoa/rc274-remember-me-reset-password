

<h1>Articles</h1>

<% @articles.each do |art| %>

<article>
  <header>
    <!-- <h3><%= art.title %></h3> -->
    <h3><%= link_to art.title, article_path(art) %></h3>
    <p class="lead">by <%= art.author %> on <%= time_tag art.updated_at.time %></p>
  </header>
  <p><%= art.content %></p>
</article>

<% end %>


